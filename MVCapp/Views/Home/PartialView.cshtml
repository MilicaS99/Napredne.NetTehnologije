@using Domain
@model MVCapp.Models.IndexViewModel

<thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone Number</th>
        <th>Email</th>
        <th>DateofRegistration</th>
    
    </tr>
</thead>
<tbody>
    @foreach (Person item in Model.Persons)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.PhoneNumber</td>
            <td>@item.Email</td>

       
        </tr>
    }
</tbody>